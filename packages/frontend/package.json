{
  "name": "pages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check ."
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@mantine/core": "6.0.21",
    "@mantine/hooks": "6.0.21",
    "@mantine/next": "6.0.21",
    "next": "13.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "2.0.32",
    "@slashnephy/prettier-config": "1.0.20",
    "@types/node": "20.8.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "concurrently": "8.2.1",
    "eslint": "8.51.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.6.4",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "rules": {
      "react-refresh/only-export-components": "off"
    }
  }
}
